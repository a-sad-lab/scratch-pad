{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "esnext",
    "experimentalDecorators": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "types": [
      "node"
    ],
    "outDir": "dist",
    "sourceMap": true
  }
}
